//Type 1 hello world 
//comments can by 2 types
//one line comment and multi line comment
// 1 line is represented by // and multi line is represented by /* (statements1) (statement2) ..... */
#include <stdio.h>

int main() {
    // Write C code here
    printf("Hello world");

    return 0;
}

//output:Hello world

//type 2 hello world
#include <stdio.h>

int main(void)
{
  printf("Hello ");
  printf("World!");
  printf("\n");
}

//output: Hello World!

//type 3 helloworld

#include <stdio.h>
int main(void)
{
  int i = 0;
  char *str = "Hello World!\n";

 /* Print each character until reach ’\0’ */
  while (str[i] != '\0')
  printf("%c", str[i++]);

 return 0;
}


//output:Hello World!
